programa 
{
	funcao inicio()
	{
		real x,y 
		
		escreva("Digite o primeiro número: ")
		leia(x)
		escreva("Digite o segundo número: ")
		leia(y)
		
		se(x > y){
		escreva ("O número ",x," é o maior")
		}
		    senao se(x < y)
		    escreva ("O número ",y," é o maior")
		    senao
		    escreva ("Os números são equivalentes")
	}
}
